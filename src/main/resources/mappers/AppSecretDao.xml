<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.iqilu.message.transfer.dao.AppSecretDao">

    <resultMap id="toAppSecret" type="com.iqilu.message.transfer.pojo.AppSecret">
        <id property="id" column="id"/>
        <result property="appId" column="app_id"/>
        <result property="secret" column="secret_value"/>
    </resultMap>


    <select id="listAllAppSecrets" resultMap="toAppSecret">
        select id, app_id, secret_value from tb_secret
    </select>


</mapper>